               <ImageView fitHeight="90.0" fitWidth="140.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true">
                  <StackPane.margin>
                     <Insets bottom="10.0" top="15.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/position_off.png" />
               </ImageView>
               <ImageView fx:id="positionRight" fitHeight="90.0" fitWidth="140.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true" visible="false">
                  <StackPane.margin>
                     <Insets bottom="10.0" top="15.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/position_on.png" />
               </ImageView>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <ImageView fitHeight="90.0" fitWidth="140.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true">
                  <StackPane.margin>
                     <Insets bottom="10.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/fog_off.png" />
               </ImageView>
               <ImageView fx:id="fogRight" fitHeight="90.0" fitWidth="140.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true" visible="false">
                  <StackPane.margin>
                     <Insets bottom="10.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/fog_on.png" />
               </ImageView>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <ImageView fitHeight="90.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
                  <StackPane.margin>
                     <Insets bottom="10.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/road_off.png" />
               </ImageView>
               <ImageView fx:id="roadLight" fitHeight="90.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true" visible="false">
                  <StackPane.margin>
                     <Insets bottom="10.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/road_on.png" />
               </ImageView>
            </StackPane>
         </VBox>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
