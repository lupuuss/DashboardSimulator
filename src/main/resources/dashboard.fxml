            <opaqueInsets>
               <Insets />
            </opaqueInsets>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <ImageView fitHeight="90.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
                  <StackPane.margin>
                     <Insets bottom="10.0" right="15.0" top="15.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/position_off.png" />
               </ImageView>
               <ImageView fx:id="positionLeft" fitHeight="90.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true" visible="false">
                  <StackPane.margin>
                     <Insets bottom="10.0" right="15.0" top="15.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/position_on.png" />
               </ImageView>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <ImageView fx:id="fogLeft" fitHeight="90.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
                  <StackPane.margin>
                     <Insets bottom="10.0" right="15.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/fog_off.png" />
               </ImageView>
               <ImageView fitHeight="90.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true" visible="false">
                  <StackPane.margin>
                     <Insets bottom="10.0" right="15.0" />
                  </StackPane.margin>
                  <Image url="@./imgs/fog_on.png" />
