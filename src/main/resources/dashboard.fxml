   <VBox prefHeight="400.0" prefWidth="257.0">
      <Label style="-fx-label-padding: 15; -fx-text-fill: #f0f8ff;" text="Prędkość maksymalna">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label style="-fx-label-padding: 15; -fx-text-fill: #f0f8ff;" text="Prędkość średnia">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label style="-fx-label-padding: 15; -fx-text-fill: #f0f8ff;" text="Czas podróży">
         <font>
            <Font size="23.0" />
