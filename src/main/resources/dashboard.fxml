         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label fx:id="distancePassed" style="-fx-label-padding: 15; -fx-text-fill: #f0f8ff;" text="0">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label fx:id="avgConsumption" style="-fx-label-padding: 15; -fx-text-fill: #f0f8ff;" text="0">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
   </VBox>
</HBox>
