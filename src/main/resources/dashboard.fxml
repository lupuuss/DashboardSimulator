               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <Label style="-fx-text-fill: #f0f8ff;" text="Dailty odometer">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
         </VBox>
         <VBox prefHeight="200.0" prefWidth="100.0">
            <Label fx:id="odometer" style="-fx-text-fill: #f0f8ff;" text="0">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <Label fx:id="dailyOdometer" style="-fx-text-fill: #f0f8ff;" text="0">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
         </VBox>
      </HBox>
